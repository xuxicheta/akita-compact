{
  "$schema": "http://json-schema.org/schema",
  "id": "StoreSchematics",
  "title": "Hello Options",
  "type": "object",
  "description": "Store Schematics",
  "properties": {
    "name": {
      "type": "string",
      "description": "Name",
      "$default": {
        "$source": "argv",
        "index": 0
      },
      "x-prompt": "name?"
    },
    "active": {
      "type": "boolean",
      "description": "Has active?",
      "default": false,
      "x-prompt": "active?"
    },
    "idKey": {
      "type": "string",
      "description": "id key",
      "x-prompt": "id key?"
    },
    "project": {
      "type": "string",
      "description": "The name of the project.",
      "$default": {
        "$source": "projectName"
      }
    }
  },
  "required": [
    "name",
    "idKey"
  ]
}